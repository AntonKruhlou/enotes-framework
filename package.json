{
  "name": "enotes-automation-framework",
  "version": "1.0.0",
  "description": "Project to automate Enotes UI web",
  "scripts": {
    "code:formatter": "eslint . --fix && prettier . --check --write",
    "prepare": "husky install",
    "report:generate": "allure generate --clean ./reports/allure-results",
    "report:open": "allure generate --clean ./reports/allure-results && allure open",
    "test:web:local:chrome": "cross-env wdio run test/wdio/configs/wdio/local/web/chrome.ts",
    "test:web:local:safari": "cross-env wdio run test/wdio/configs/wdio/local/web/safari.ts"
  },
  "devDependencies": {
    "@types/chai": "^4.3.10",
    "@typescript-eslint/eslint-plugin": "^6.11.0",
    "@typescript-eslint/parser": "^6.11.0",
    "@wdio/allure-reporter": "^7.32.0",
    "@wdio/cli": "^7.32.3",
    "@wdio/devtools-service": "^7.32.3",
    "@wdio/local-runner": "^7.32.3",
    "@wdio/mocha-framework": "^7.30.2",
    "@wdio/spec-reporter": "^7.31.1",
    "allure-commandline": "^2.24.1",
    "chai": "^4.3.10",
    "chalk": "^5.3.0",
    "dayjs": "^1.11.10",
    "chromedriver": "^130.0.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.54.0",
    "eslint-plugin-mocha": "^10.5.0",
    "eslint-plugin-wdio": "^7.4.2",
    "husky": "^8.0.3",
    "jwt-decode": "^4.0.0",
    "npm-check-updates": "^16.14.11",
    "prettier": "^3.1.0",
    "qs": "^6.11.2",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2",
    "wdio-chromedriver-service": "^7.3.2",
    "wdio-safaridriver-service": "^2.1.1",
    "webdriverio": "^7.32.3",
    "winston": "^3.11.0"
  }
}
